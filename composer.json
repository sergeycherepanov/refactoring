{
    "name": "sergeycherepanov/refactoring",
    "description": "The Example Refactoring App",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "symfony/console": "6.4.x-dev",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/dotenv": "6.4.x-dev",
        "league/flysystem": "^3.0",
        "kevinrob/guzzle-cache-middleware": "^5.0",
        "brick/money": "dev-master"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "scherepanov",
            "email": "s@cherepanov.co"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "2.0.x-dev",
        "squizlabs/php_codesniffer": "^3",
        "friendsofphp/php-cs-fixer": "dev-master"
    }
}
